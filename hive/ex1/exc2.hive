drop table A;
drop table B;
drop table final;

create table A (nm String, dp String, id String)
ROW FORMAT DELIMITED  FIELDS TERMINATED BY ',' ;

create table B (id String, dt String, hr int)
ROW FORMAT DELIMITED  FIELDS TERMINATED BY ',' ;

create table final (dp String, id String , nm String, avg float)
ROW FORMAT DELIMITED  FIELDS TERMINATED BY ',' ;

load data inpath 'input/h1/file1.txt' into table A;
load data inpath 'input/h1/file2.ok.txt' into table B;

insert overwrite table final select a.id, collect_set(a.dp),  collect_set(a.nm), avg(b.hr) from a,b where b.hr > 8 and b.id = a.id group by a.id;

select * from  final;
